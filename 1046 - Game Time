#include <iostream>
using namespace std;

int main() {
    int x,y,r;
    cin>>x>>y;
    if(x<25 && y<25 && x>=0 && y>=0){
        if(y>x){
            r= y-x;
        }else if(x>y)
        {
            y=y+24;
            r = y-x;
        }else{
            r=24;
        }
    }
    cout<<"O JOGO DUROU " <<r<<" HORA(S)"<<endl;

    return 0;
}
