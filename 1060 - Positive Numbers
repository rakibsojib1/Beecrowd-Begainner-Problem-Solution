#include <iostream>
using namespace std;
int main() {
    double a,b,c,d,e,f;
    int count =0;
    cin>>a>>b>>c>>d>>e>>f;
    double arr[6]={a,b,c,d,e,f};
   for(int i=0;i<6;i++){
       if(arr[i]>=0){
           count++;
       }
   }
    cout<<count<<" valores positivos"<<endl;
    
    return 0;
}
