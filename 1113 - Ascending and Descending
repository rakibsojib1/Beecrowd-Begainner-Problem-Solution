#include<iostream>
using namespace std;
int main(){
    int x,y;
    //cin>>x>>y;
    while(x!=y){
        cin>>x>>y;
        if(x>y){
            cout<<"Decrescente"<<endl;
        }else if(y>x){
            cout<<"Crescente"<<endl;
        }else{
            break;
        }
    }
    return 0;
}
